  }
  .moreShops{
    width:100%;
    height:.75rem;
    line-height:.75rem;
    font-size:.24rem;
    clear:both;
    .icon{
      color:#999;
      float:right;
    }
    a{
      color:#333;
    }
  }
}

.add-cart{
  width:100%;
  height:.7rem;
  line-height:.7rem;
  overflow:hidden;
  background:#fff;
  position:fixed;
  bottom:0px;
  box-shadow: 0px -1px 10px #ccc;
  input{
    width:.7rem;
    height:.45rem;
    line-height:.45rem;
    border:none;
    border-top:#ccc solid 1px;
    border-bottom:#ccc solid 1px;
    color:$priceColor;
    font-size:.24rem;
    float:left;
    text-align:center;
    margin-top:.13rem;
  }
  .sub{
    width:.45rem;
    height:.45rem;
    line-height:.45rem;
    font-size:.24rem;
    color:#999;
    border:#ccc solid 1px;
    float:left;
    text-align:center;
    margin:.13rem 0 0 .42rem;
  }
  .add{
    width:.45rem;
    height:.45rem;
    line-height:.45rem;
    font-size:.24rem;
    color:#999;
    border:#ccc solid 1px;
    float:left;
    text-align:center;
    margin:.13rem 0 .42rem 0;
  }
  .addBtn{
    width:1.9rem;
    height:.7rem;
    line-height:.7rem;
    overflow:hidden;
    font-size:.24rem;
    color:#8ec200;
    background:#e8f3cc;
    text-align:center;
    float:right;
  }
  .buyBtn{
    width:1.9rem;
    height:.7rem;
    line-height:.7rem;
    overflow:hidden;
    font-size:.24rem;
    color:#fff;
    background:$bgColor;
    text-align:center;
    float:right;
  }
  .noBuyBtn{
    width:3.8rem;
    height:.7rem;
    line-height:.7rem;
    font-size:.24rem;
    color:#fff;
    background:#ccc;
    text-align:center;
    float:right;
  }
}

.option-cart{
  width:100%;
  height:.7rem;
  line-height:.7rem;
  overflow:hidden;
  background:#fff;
  position:fixed;
  bottom:0px;
  box-shadow: 0px -1px 10px #ccc;
  .left{
    width:5%;
    float:left;
    margin:0 .1rem;

  }
  span{
    font-size:.24rem;
    color:#333;
    margin-right:.1rem;
    em{
      color:$bgColor;
    }
    a{color:#333;}
  }
  .order-cart{
    width:4.9rem;
    float:left;
    margin-left:.2rem;
    .right-price{
      float:right;
    }
  }
  .right{
    width:1.3rem;
    height:.7rem;
    line-height:.7rem;
    float:right;
    margin-right:0;
    text-align:center;
    color:#fff;
    background:$bgColor;
    em{
      color:#fff;
    }
    a{color:#fff;}
  }
}

#warp_content{
  width:6rem;
  margin:0 .2rem;
  .content{
    font-size:.24rem;
    line-height:150%;
    padding:.2rem 0;
    img{
      width:100%;
      margin-top:.2rem;
    }
  }
}

.pay-submit{
  width:4.8rem;
  height:0.8rem;
  line-height:.8rem;
  font-size:.3rem;
  color:#fff;
  text-align:center;
  background:$bgColor;
  display:block;
  margin:0.6rem;
}

.pay-result{
  width:100%;
  height:2.35rem;
  overflow:hidden;
  font-size:.24rem;
  color:#999;
  padding:.6rem 0;
  .left{
    width:30%;
    height:auto;
    overflow:hidden;
    float:left;
    .icon{
      font-size:.8rem;
      font-weight:bold;
      color:#9edc74;
      float:right;
    }
  }
  .right{
    width:60%;
    float:left;
    margin-left:.3rem;
    p{
      line-height:200%;
      i{
        color:$priceColor;
      }
      em{
        font-size:.38rem;
        color:$priceColor;
      }
    }
  }
}

#pages{
  width:5rem;
  height:.48rem;
  line-height:.48rem;
  margin:.4rem .7rem 0 .7rem;
  .prevPage{
    font-size:.24rem;
