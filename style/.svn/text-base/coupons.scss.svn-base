  span{
    width:1.4rem;
    text-align:center;
    color:#999;
    float:left;
    font-size:.24rem;
    line-height:200%;
    display:block;
    float:left;
  }
}

.result{
  width:6rem;
  height:.7rem;
  line-height:.7rem;
  margin:0 .2rem;
  color:#666;
  font-size:.24rem;
  em{
    color:$bgColor;
  }
}

.no-result{
  width:6rem;
  height:.8rem;
  line-height:.8rem;
  margin:0 .2rem;
  color:#999;
  font-size:.24rem;
}

.footer{
  width:100%;
  height:auto;
  overflow:hidden;
  .action{
    width:6rem;
    height:.7rem;
    line-height:.7rem;
    margin:.36rem .2rem;
    height:auto;
    overflow:hidden;
    background:#fff;
    a{
      float:left;
      padding:0 .2rem;
      font-size:.24rem;
      color:#0d0d0d;
    }
    .toTop{
      float:right;
      color:#0d0d0d;
      .icon{
        color:#dfdfdf;
        font-style:normal;
        text-decoration: none;
      }
    }
  }

  .screen{
    width:5.2rem;
    height:.6rem;
    margin:0 .6rem;
    height:auto;
    overflow:hidden;
    a{
      width:1.4rem;
      height:.6rem;
      font-size:.24rem;
      text-align:center;
      line-height:.6rem;
      float:left;
      margin-right:.5rem;
      color:#0d0d0d;
      background:#e6e6e6;
