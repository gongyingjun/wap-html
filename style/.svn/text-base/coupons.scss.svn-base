$bgColor : #ff8822;
$priceColor: #f9403d;

body{background:#f5f5f5;}
.header{
  width: 100%;
  height:0.8rem;
  background:$bgColor;
  overflow:hidden;
  .content{
    width: 70%;
    text-align:center;
    color:#fff;
    font-size:0.3rem;
    line-height:0.8rem;
    margin:auto;
    border:#000 solid 0px;
    float:left;
    margin-left:15%;
    position:absolute;
  }
  .back{
    font-size:.4rem;
    line-height:.8rem;
    float:left;
    margin-left:.2rem;
    width:15%;
  }
  .cart{
    font-size:.4rem;
    float:right;
    line-height:60%;
    text-align:center;
    margin-top:.2rem;
    position:relative;
    width:20%;
    s{
      font-style:normal;
      text-decoration: none;
      font-size:0.14rem;
      line-height:100%;
      font-weight:bold;
    }
  }
  .right{
    font-size:.28rem;
    line-height:300%;
    float:right;
    margin-right:.2rem;
    color:#fff;
  }
}

.coupons-list{
  width:100%;
  height:auto;
  line-height:.4rem;
  margin:auto;
  overflow:hidden;
  background:#f5f5f5;
  .title{
    font-size:.35rem;
    color:#333;
    border-left: $priceColor solid 0.03rem;
    text-indent:.1rem;
    margin:.2rem;
  }
  .other-title{
    width:100%;
    height:auto;
    overflow:hidden;
    background:#fff;
    border-bottom:#dfdfdf solid 1px;
    h4{
      width:6rem;
      height:.55rem;
      line-height:.55rem;
      margin:0 .2rem;
      font-size:.24rem;
      color:#999;
      font-weight:normal;
    }
  }
  .lists{
    margin:auto;
    width:100%;
    height:auto;
    overflow:hidden;
    li{
      width:100%;
      height:auto;
      overflow:hidden;
      margin-bottom:.02rem;
      padding:.2rem 0;
      background:#fff;
      .left{
        width:2.3rem;
        height:1.5rem;
        margin-left:.2rem;
        float:left;
        img{width:100%;}
      }
      .right{
        width:3.5rem;
        float:right;
        margin-right:.2rem;
        h2{
          width:100%;
          height:.58rem;
          color:#333;
          line-height:.24rem;
          font-size:.24rem;
          font-weight:normal;
          overflow:hidden;
          white-space: nowrap;
          text-overflow:ellipsis;
        }
        .p1{
          width:100%;
          height:.46rem;
          overflow:hidden;
          .price{

            width:1rem;
            color:$priceColor;
            font-size:.3rem;
            line-height:.3rem;
            display:block;
            float:left;
            margin-top:.18rem;
            em{
              font-size:.18rem;
            }
            s{
              color:#999;
              font-size:.18rem;
              margin-left:5%;
            }
          }
          .price-old{
              color:#999;
              font-size:.18rem;
              float:left;
              margin-top:.18rem;
              text-decoration:line-through;
          }
          .grab-btn{
            width:1.4rem;
            height:.46rem;
            line-height:.46rem;
            background:$bgColor;
            font-size:.24rem;
            float:right;
            color:#fff;
            text-align:center;
          }
        }
        .p2{
          width:100%;
          height:.18rem;
          line-height:.18rem;
          margin-top:.28rem;
          .timer{
            color:#999;
            font-size:.18rem;
            em{
              color:$bgColor;
            }
          }
          .timer-grab{
            color:#8ec200;
            font-size:.18rem;
          }
          .sale-result{
            color:#999;
            font-size:.18rem;
            em{
              color:$bgColor;
            }
          }
          .sold{
            float:right;
            color:#999;
            font-size:.18rem;
          }
          .grab-notice-btn{
            width:1.4rem;
            height:.46rem;
            line-height:.46rem;
            background:#e8f3cc;
            font-size:.24rem;
            float:right;
            color:#8ec200;
            text-align:center;
            margin-top:-0.3rem;
          }
          .addCart-btn{
            width:1.4rem;
            height:.46rem;
            line-height:.46rem;
            background:$bgColor;
            font-size:.24rem;
            float:right;
            color:#fff;
            text-align:center;
            margin-top:-0.3rem;
          }
        }
      }
    }
  }
}

.buy-list{
  width:100%;
  margin:auto;
  height:auto;
  overflow:hidden;
  background:#f5f5f5;
  .other-title{
    width:100%;
    height:auto;
    overflow:hidden;
    h4{
      width:6rem;
      height:.6rem;
      line-height:.6rem;
      margin:0 .2rem;
      font-size:.24rem;
      color:#999;
      font-weight:normal;
    }
  }
  .lists{
    margin:auto;
    width:93.65%;
    height:auto;
    overflow:hidden;
    li{
      width:2.9rem;
      display:inline;
      float:left;
      border:#000 solid 0px;
      height:auto;
      overflow:hidden;
      margin-bottom:.02rem;
      &:nth-child(2n) {
        display:inline;
        float:right;
      }
      img{width:100%;}
      .left{
        width:100%;
        height:auto;
        overflow:hidden;
      }
      .right{
        width:100%;
        height:auto;
        overflow:hidden;
        padding:.2rem 0;
        h2{
          font-size:.24rem;
          line-height:.5rem;
          color:#333;
          font-weight:normal;
          width:100%;
          overflow:hidden;
          text-overflow:ellipsis;
          white-space: nowrap;
        }
        .price{
          float:left;
          color:$priceColor;
          margin-top:-0.1rem;
          em{
            font-size:.18rem;
          }
          i{
            font-size:.3rem;
          }
        }
        .sale-result{
          float:right;
          color:#999;
          font-size:.18rem;
        }
      }
    }
  }
}

.order-menu{
  width:100%;
  height:.65rem;
  height:auto;
  overflow:hidden;
  background:#fff;
  .lists{
    width:6rem;
    height:auto;
    overflow:hidden;
    margin:0 .2rem;
    li{
      width:25%;
      float:left;
      display:inline;
      text-align:center;
      a{
        color:#666;
        font-size:.24rem;
        line-height:250%;
        display:block;
      }
      .active{
        color:$bgColor;
        border-bottom:$bgColor solid 2px;
      }
    }
  }
}

.search{
  widht:6rem;
  height:1rem;
  overflow:hidden;
  margin:0 .2rem;
  .search-input{
    width:4.48rem;
    height:.6rem;
    overflow:hidden;
    font-size:.24rem;
    color:#666;
    line-height:.6rem;
    text-indent:.1rem;
    border:#ccc solid 1px;
    margin:.2rem auto;
    &::-webkit-input-placeholder {
      font-size:.24rem;
      color:#999;
      text-indent:.1rem;
    }
  }
  .search-btn{
    width:1.3rem;
    height:.6rem;
    line-height:.6rem;
    font-size:.3rem;
    background:$bgColor;
    color:#fff;
    text-align:center;
    float:right;
    margin:.2rem auto;
  }
}

.table-header{
  width:100%;
  height:.8rem;
  line-height:.8rem;
  overflow:hidden;
  background:#fff;
  .menu{
    width:6rem;
    margin:0 .2rem;
    list-style:none;
    li{
      float:left;
      display:inline;
      width:30%;
      text-align:center;
      font-size:.3rem;
      color:#666;
      &:first-child{
        text-align:left;
        width:20%;
      }
      &:last-child{
        text-align:right;
        width:20%;
      }
      .icon{
        color:#666;
      }
    }
    .active{
      color:$bgColor;
    }
  }
  .menu-list{
    width:100%;
    list-style:none;
    height:auto;
    overflow:hidden;
    border-top:#f5f5f5 solid 1px;
    position:absolute;
    background:#fff;
    z-index:999;
    margin-top:0.8rem;
    display:none;
    li{
      float:left;
      display:inline;
      width:6rem;
      height:.8rem;
      line-height:.8rem;
      margin:0 .2rem;
      text-align:left;
      font-size:.24rem;
      color:#999;
      border-bottom:#f5f5f5 solid 1px;
      &:last-child{
        border-bottom:none;
      }
    }
    .active{
      float:left;
      display:inline;
      width:4.5rem;
      text-align:left;
      font-size:.24rem;
      color:#333;
      border-bottom:#f5f5f5 solid 1px;
      &:last-child{
        border-bottom:none;
      }
      .icon{
        color:$bgColor;
        font-size:.42rem;
        float:right;
      }
    }
    .left{
      width:1.6rem;
      float:left;
      margin:0;
      ul{
        width:100%;
        background:#f5f5f5;
        height:auto;
        overflow:hidden;
        li{
          margin-left:.2rem;
          color:#999;
        }
        .active{
          color:#666;
          position:relative;
          .icon{
            font-size:.62rem;
            color:#fff;
            position:absolute;
            left:1rem;
          }
        }
      }
    }
    .right{
      width:4.8rem;
      height:auto;
      overflow:hidden;
      float:right;
      .menu-child{
        height:auto;
        overflow:hidden;
        display:none;
        li{
          float:left;
          display:inline;
          width:4rem;
          height:.8rem;
          line-height:.8rem;
          margin:0 .4rem;
          text-align:left;
          font-size:.24rem;
          color:#999;
          border-bottom:#f5f5f5 solid 1px;
          &:last-child{
            border-bottom:none;
          }
        }
        .active{
          float:left;
          display:inline;
          width:4rem;
          margin:0 .4rem;
          text-align:left;
          font-size:.24rem;
          color:#333;
          border-bottom:#f5f5f5 solid 1px;
          &:last-child{
            border-bottom:none;
          }
          .icon{
            color:$bgColor;
            font-size:.42rem;
            float:right;
          }
        }
      }
    }
  }
}

.menu-more-list{
  width:100%;
  list-style:none;
  height:auto;
  overflow:hidden;
  border-top:#f5f5f5 solid 1px;
  background:#fff;
  z-index:999;
  margin-top:.7rem;
  display:none;
  z-index:9999;
  position:absolute;
  li{
    float:left;
    display:inline;
    width:6rem;
    height:.8rem;
    line-height:.8rem;
    margin:0 .2rem;
    text-align:left;
    font-size:.24rem;
    color:#333;
  }

  .coupons-choose{
    float:left;
    display:inline;
    width:2.8rem;
    height:.6rem;
    line-height:.6rem;
    text-align:center;
    font-size:.24rem;
    color:#999;
    background:#f1f1f1;
    margin-bottom:.2rem;
  }

  .active{
    color:$bgColor;
  }
}


.shop-list{
  width:100%;
  margin:auto;
  height:auto;
  overflow:hidden;
  background:#f5f5f5;
  .lists{
    margin:auto;
    width:100%;
    height:auto;
    overflow:hidden;
    li{
      width:100%;
      border:#000 solid 0px;
      height:auto;
      overflow:hidden;
      margin-bottom:.2rem;
      padding:0.2rem 0;
      background:#fff;
      &:last-child{margin-bottom:0rem;}
      .left{
        width:4.9rem;
        margin-left:.2rem;
        float:left;
        h2{
          font-size:.24rem;
          line-height:200%;
          font-weight:normal;
          white-space: nowrap;
          width:100%;
          height:auto;
          overflow:hidden;
          text-overflow:ellipsis;
        }
        .address{
          font-size:.24rem;
          color:#999;
          font-weight:normal;
          white-space: nowrap;
          width:100%;
          height:.48rem;
          line-heght:.48rem;
          overflow:hidden;
          text-overflow:ellipsis;
        }
        .distance{
          font-size:.24rem;
          color:#999;
          height:.24rem;
          line-height:.24rem;
          overflow:hidden;
          font-weight:normal;
          white-space: nowrap;
          width:100%;
          text-overflow:ellipsis;
          .icon{
            display:block;
            float:left;
            font-size:.24rem;
            color:#999;
            width:0.3rem;
            margin-top:-0.02rem;
          }
        }
      }
      .right{
        width:1.3rem;
        height:1.1rem;
        line-height:.9rem;
        overflow:hidden;
        float:right;
        color:$bgColor;
        border:#000 solid 0px;
        .icon{
          color:$bgColor;
          font-size:.72rem;
          font-weight:blod;
          border-left:#dfdfdf solid 1px;
          display:block;
          text-align:center;
          margin:0.2rem 0;
        }
      }
    }
  }
}

